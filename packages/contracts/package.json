{
  "name": "@coolcinema/contracts",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "protos"
  ],
  "scripts": {
    "build": "pnpm run generate && tsc",
    "generate": "mkdir -p src/generated && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated --ts_proto_opt=outputServices=grpc-js,env=node,esModuleInterop=true -I ./protos ./protos/*.proto"
  },
  "peerDependencies": {
    "@grpc/grpc-js": "^1.14.3"
  },
  "devDependencies": {
    "ts-proto": "^2.6.1",
    "typescript": "^5.9.3",
    "@types/node": "^25.0.9",
    "@grpc/grpc-js": "^1.14.3"
  }
}
